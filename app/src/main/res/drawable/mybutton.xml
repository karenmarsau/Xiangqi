<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="50dip" />
            <stroke android:width="2dip" android:color="#B06933" />
            <gradient android:angle="-90" android:startColor="#B06933" android:endColor="#B06933"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="50dip" />
            <stroke android:width="1dip" android:color="#B06933" />
            <solid android:color="#B06933"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="50dip" />
            <stroke android:width="2dip" android:color="#834415" />
            <gradient android:angle="-90" android:startColor="#834415" android:endColor="#834415" />
        </shape>
    </item>
</selector>
